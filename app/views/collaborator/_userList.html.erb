<!-- ":private" is being used for collaborator -->
<div class="col-md-4">
  <% if current_user.admin? || current_user.id == @wiki.user_id? %>
    <%= @wiki.collaborators(@users).each do |w| %>
      <div class="form-group">
        <%= w.label :private, class: 'checkbox' do %>
          <%= w.check_box :private %> <%= @wiki.collaborator %>
        <% end %>
      </div>
    <% end %>
  <% end %>
</div>
